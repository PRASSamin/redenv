{
  "name": "@redenv/client",
  "version": "1.0.6",
  "description": "A lightweight, caching client for securely fetching secrets from Redenv in any server-side JavaScript or TypeScript application.",
  "author": {
    "name": "PRAS",
    "email": "prassamin@gmail.com",
    "url": "https://pras.me"
  },
  "license": "MIT",
  "keywords": [
    "client",
    "sdk",
    "env",
    "secrets",
    "security",
    "upstash",
    "redis",
    "dotenv",
    "caching",
    "serverless",
    "edge"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/PRASSamin/redenv.git"
  },
  "type": "module",
  "scripts": {
    "build": "tsup"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist",
    "CHANGELOG.md"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "import": "./dist/index.js",
        "types": "./dist/index.d.ts"
      },
      "require": {
        "require": "./dist/index.cjs",
        "types": "./dist/index.d.cts"
      }
    },
    "./utils": {
      "import": {
        "import": "./dist/utils.js",
        "types": "./dist/utils.d.ts"
      },
      "require": {
        "require": "./dist/utils.cjs",
        "types": "./dist/utils.d.cts"
      }
    }
  },
  "dependencies": {
    "@epic-web/cachified": "^5.6.0",
    "@redenv/core": "workspace:*",
    "@upstash/redis": "^1.35.6",
    "lru-cache": "^11.2.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "typescript": "^5"
  }
}
